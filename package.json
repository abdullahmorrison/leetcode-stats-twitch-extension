{
  "name": "leetcode-stats-twitch-extension",
  "version": "1.0.0",
  "description": "Twitch extension that displays Abdullah Morrison's LeetCode stats",
  "main": "src/index.tsx",
  "scripts": {
    "start": "npm-run-all --parallel sass:watch serve",
    "serve": "webpack serve",
    "sass:build": "sass --no-source-map src:src",
    "sass:watch": "npm run sass:build -- --watch",
    "build": "set NODE_ENV=production && webpack",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abdullahmorrison/leetcode-stats-twitch-extension.git"
  },
  "keywords": [
    "LeetCode",
    "Twitch",
    "Abdullah"
  ],
  "author": "Abdullah Morrison",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/abdullahmorrison/leetcode-stats-twitch-extension/issues"
  },
  "homepage": "https://github.com/abdullahmorrison/leetcode-stats-twitch-extension#readme",
  "devDependencies": {
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "css-loader": "^7.1.2",
    "html-webpack-plugin": "^5.6.3",
    "mini-css-extract-plugin": "^2.9.2",
    "ts-loader": "^9.5.2",
    "typescript": "^5.7.3",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.66.0",
    "npm-run-all": "^4.1.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sass": "^1.83.4"
  }
}
